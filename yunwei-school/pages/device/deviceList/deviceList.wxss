/* pages/device/deviceList/deviceList.wxss */
page{
  height:100%;
}
.container{
  padding: 20rpx 0;
}
.deviceList{
  width:100%;
  height:100%;
  border-top: 1rpx solid #f1f1f1;
}
.deviceList .item{
  width: 100%;
  height:100rpx;
  display: flex;
  justify-content: space-between;
  line-height:100rpx;
  background-color: #fff;
  padding: 0 20rpx;
  border-bottom: 1rpx solid #f1f1f1;
}
.deviceList .item .num{
  color:#868d98;
  padding-left: 6rpx;
}
.deviceList .item .status{
  width: 30rpx;
  height:30rpx;
  margin: 35rpx 10rpx;
}
.deviceList .item .more{
  width: 30rpx;
  height:40rpx;
  margin: 30rpx 10rpx;
}